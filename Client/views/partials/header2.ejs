    <style>
      @import url('../../css/style2.css');
    </style>
    <section class="chapter-navigation">
      <div class="nav-chapter-logo2">
        <img src="../../images/iare_header_logo.png" alt="iare-logo">
      </div>
      <div class="chapter-nav-btn">
        <button class="nav-btn"><a href="/">Home</a></button>
        <% if(imgUsername){ %>
          <button class="nav-btn"><a href="/contest">BuildIT Lab</a></button>
          <button class="nav-btn"><a href="/logout">Logout</a></button>
          <button class="nav-btn" style="color: white; font-weight: bold; font-family: poppins;" id="username"><%= imgUsername %></button>
          <span style="margin-right: 20px;"><img src="<%=imgUrl%>" alt="img" style="width: 80px; height: 80px; border-radius: 1mm;"></span>         
        <% } else { %>
          <button class="nav-btn"><a href="/login">Login</a></button>
        <% } %>
      </div>
      <button type="button" class="mbutton" id="menu-open"><span class="material-icons">menu</span></button>
    </section>
    <section class="chapter-navigation2">
      <div class="topbar">
        <div class="nav2images">
          <img src="../../images/iare-chapter/iare-logo.png" alt="iare-logo">
        </div>
        <button type="button" class="mbutton" id="menu-close"><span class="material-icons">close</span></button>
      </div>
      <div class="chapter-nav-btn2">
        <button class="nav-btn2" onclick="navslide()"><a href="#">Home <span class="material-icons">arrow_forward<span></a></button>
          <% if (imgUsername){ %>
        <button class="nav-btn2" onclick="navslide()"><a href="/contest">BuildIT Lab <span class="material-icons">arrow_forward<span></span></a></button>
        <button class="nav-btn2" onclick="navslide()"><a href="/profile">Profile <span class="material-icons">arrow_forward<span></span></a></button>
        <button class="nav-btn2" onclick="navslide()"><a href="/logout">Logout <span class="material-icons">arrow_forward<span></span></a></button>
        <% } else { %>
          <button class="nav-btn2" onclick="navslide()"><a href="/login">Login <span class="material-icons">arrow_forward<span></span></a></button>
        <% } %>
      </div>
      <div class="chnav2end">
        <div class="footer-left-titles">
        <a href="http://buildit.iare.ac.in/" target="_blank">Build-IT</a>
      </div>
        <button class="nav-btn3"><a href="#footers">Feedback</a></button>
      </div>
    </section>
        <script>
      var ri = document.querySelectorAll(".ri");
      for (var i = 0; i < ri.length; i++) {
        ri[i].style.color = "rgba(120, 120, 120,0.7)";
      }
    </script>
    <script>
      var mop = document.getElementById("menu-open");
      var mcl = document.getElementById("menu-close");
      var chna2 = document.querySelector(".chapter-navigation2");
      chna2.style.transform = "translateX(101vw)";
      function navslide() {
        chna2.style.transform = "translateX(101vw)";
      }
      mop.onclick = function () {
        chna2.style.transform = "translateX(0)";
      };
      mcl.onclick = function () {
        chna2.style.transform = "translateX(101vw)";
      };
      var navigation = document.querySelector(".chapter-navigation");
      var th = document.body.scrollHeight - window.innerHeight;
      var ij = 0;
      navigation.style.background = "#41228e";
      window.onscroll = function () {
        let p =
          (window.pageYOffset /
            (document.body.scrollHeight - window.innerHeight)) *
          100;
        if (p > 5) {
          navigation.style.transition = "all 0.25s";
          navigation.style.boxShadow = "0 0 10px rgba(0,0,0,0)";
        } else {
          navigation.style.transition = "all 0.25s";
          navigation.style.boxShadow = "0 0 0px rgba(0,0,0,0)";
        }
        if (p > ij && p > 15) {
          navigation.style.transform = "translateY(0px)";
        } else if (p < ij) {
          navigation.style.transform = "translateY(0px)";
        }
        ij = p;
        if (p > 10) {
          navigation.style.background = "rgb(166,34,50)";
        } else {
          navigation.style.background = "#41228e";
        }
      };
    </script>
